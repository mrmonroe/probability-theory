{
    "source": {
        "includePattern": ".+\\.js(doc|x)?$",   
        "include": ["."],                       
        "excludePattern": "(node_modules/|docs)"           
    },
    "recurseDepth": 10,                        
    "opts": {
        "destination": "./docs/",              
        "recurse": true,
        "private": true,
        "encoding": "utf8",                         
        "template": "node_modules/braintree-jsdoc-template"                       
    },
    "plugins": [
        "plugins/markdown"
    ],
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc"]
    },
    "templates": {
        "default": {
            "outputSourceFiles": false
        },
        "referenceTitle": "Probability Theory",
        "disableSort": false,
        "collapse": true,
        "resources": {
            "Poisson Distribution": "https://en.wikipedia.org/wiki/Poisson_distribution"
            
        }
    }
}